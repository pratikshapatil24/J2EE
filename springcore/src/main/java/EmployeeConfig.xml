<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
     <bean class="com.jspider.springcore.manytoon.EmployeeBean" name="Employee1">
     <property name="id" value="1"/>
     <property name="name" value="Ram"/>
     <property name="salary" value="30000"/>
     <property name="company" ref="Company1"/>
      </bean>
      
      <bean class="com.jspider.springcore.manytoon.EmployeeBean" name="Employee2">
      <property name="id" value="2"/>
     <property name="name" value="Sham"/>
     <property name="salary" value="35000"/>
     <property name="company" ref="Company1"/>
      </bean>
      
      <bean class="com.jspider.springcore.manytoon.CompanyBean" name="Company1">
      <property name="id" value="1"/>
      <property name="name" value="TCS"/>
      <property name="location" value="Mubai"/>
      </bean>


</beans>