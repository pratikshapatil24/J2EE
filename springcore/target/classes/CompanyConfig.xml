<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
  <bean class="com.jspider.springcore.beans.CompanyBean" name="Company1">
  <property name="id" value="1"/>
  <property name="name" value="Infosys"/>
  <property name="location" value="Pune"/>
 <property name="employees">
 <list>
    <bean class="com.jspider.springcore.beans.EmployeeBean" name="employee1">
    <property name="id" value="1"/>
    <property name="name" value="Suresh"/>
    <property name="salary" value="25000"/>
    </bean>
    
    <bean class="com.jspider.springcore.beans.EmployeeBean" name="employee2">
    <property name="id" value="2"/>
     <property name="name" value="Ramesh"/>
      <property name="salary" value="35000"/>
    </bean>
 </list>
 </property>
  
  </bean>
  
  
  
 

</beans>